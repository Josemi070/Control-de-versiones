#include <stdio.h>

int main() {
    int n = 8;  
    int a = 0, b = 1;

    printf("La sucesi√≥n de Fibonacci es:");
    printf("\n%d", a);
    printf("\n%d", b);

    for (int i = 2; i < n; i++) {  
        int y = a + b;
        printf("\n%d", y);
        a = b;
        b = y;
    }

    return 0;
}
